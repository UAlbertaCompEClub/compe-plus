{
  "name": "compe-plus-api",
  "version": "1.0.0",
  "description": "REST API for CompE+",
  "main": "src/server.ts",
  "scripts": {
    "prestart": "npm run build",
    "start": "node build/server.js",
    "start:dev": "nodemon --exec 'ts-node' src/server.ts",
    "build": "npm run clean && tsc",
    "clean": "rm -rf build/",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --config ../.prettierrc --ignore-path .prettierignore --write .",
    "lint": "eslint src --ext .js,.ts --max-warnings 0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.27.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "msal": "^1.4.11"
  }
}
